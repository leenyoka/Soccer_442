        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.oms.lindanyoka.soccer_442.Home"
            android:label="@string/app_name"
            android:screenOrientation="portrait" >
            <intent-filter>
                <action android:name="android.intent.action.home" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.oms.lindanyoka.soccer_442.results_activity"
            android:label="@string/app_name"
            android:screenOrientation="portrait" >
            <intent-filter>
                <action android:name="android.intent.action.results_activity" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.oms.lindanyoka.soccer_442.settings_activity"
            android:label="@string/app_name"
            android:screenOrientation="portrait" >
