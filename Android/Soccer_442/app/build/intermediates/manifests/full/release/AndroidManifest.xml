                <action android:name="android.intent.action.settings_activity" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.oms.lindanyoka.soccer_442.fixtures_activity"
            android:label="@string/app_name"
            android:screenOrientation="portrait" >
            <intent-filter>
                <action android:name="android.intent.action.fixtures_activity" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.oms.lindanyoka.soccer_442.summary_activity"
            android:label="@string/app_name"
            android:screenOrientation="portrait" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.oms.lindanyoka.soccer_442.log_activity"
            android:label="@string/app_name"
